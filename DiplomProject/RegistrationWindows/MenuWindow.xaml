<Window x:Class="DiplomProject.RegistrationWindows.MenuWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiplomProject.RegistrationWindows"
        mc:Ignorable="d"
        Title="Регистратура - меню" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Content="Выйти" FontSize="22" Margin="600,20,30,20" Click="Button_Click_1"/>
        <Button Content="Просмотр графика работы" FontSize="22" Grid.Row="1" Margin="200,20" Click="Button_Click"/>
        <Button Content="Запись приёмов" FontSize="22" Grid.Row="2" Margin="200,20" Click="Button_Click_2"/>
        <Button Content="Пациенты" FontSize="22" Grid.Row="3" Margin="200,20" Click="Button_Click_3"/>
        
    </Grid>
</Window>
